{
  "name": "@nura_zm/timeline",
  "description": "Create/learn with a digital timeline and test your knowledge.",
  "license": "MIT",
  "version": "0.1.1",
  "author": "Nura Zimmermann <nura.zimmermann@rwth-aachen.de>",
  "keywords": [
    "webwriter-widget",
    "timeline",
    "history"
  ],
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@shoelace-style/shoelace": "^2.17.1",
    "@tabler/icons": "^3.21.0",
    "@webwriter/lit": "^2.0.0",
    "@webwriter/wui": "^0.0.3",
    "lit": "^3.2.1",
    "moment": "^2.30.1"
  },
  "scripts": {
    "prepublishOnly": "npx @webwriter/build"
  },
  "exports": {
    "./widgets/webwriter-timeline.*": {
      "source": "./widgets/webwriter-timeline.ts",
      "default": "./dist/webwriter-timeline.*"
    },
    "./widgets/event-container.*": {
      "source": "./event-container.ts",
      "default": "./dist/event-container.*"
    },
    "./widgets/tl-dialog.*": {
      "source": "./tl-dialog.ts",
      "default": "./dist/tl-dialog.*"
    },
    "./widgets/event-manager.*": {
      "source": "./event-manager.ts",
      "default": "./dist/event-manager.*"
    },
    "./widgets/d-input.*": {
      "source": "./dialog-elements/d-input.ts",
      "default": "./dist/dialog-elements/d-input.*"
    },
    "./snippets/v2-women-in-tech.html": "./snippets/v2-women-in-tech.html",
    "./package.json": "./package.json"
  },
  "editingConfig": {
    "./widgets/webwriter-timeline": {
      "content": "flow*"
    },
    "./widgets/event-container": {
      "content": "flow*"
    },
    "./widgets/d-input": {
      "content": "flow*"
    },
    "./widgets/event-manager": {
      "content": "flow*"
    }
  }
}
